.new_pass{
  position: fixed;
  z-index: 50000;
  bottom: 0;
  width: 100%;
  background: #ffffff;
  border-top: 1px solid #e5e5e5;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  font-size: 32rpx;
  box-shadow: 0px -6px 10px rgba(15,39,51,0.1);
}
.new_pass image{
  vertical-align: middle;
  width: 40rpx;
  height: 40rpx;
  margin-right: 12rpx;
}
.new_pass-box{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  position: fixed;
  z-index: 50000;
  bottom: 0;
  width: 100%;
  background: #ffffff;
  border-top: 1px solid #e5e5e5;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  font-size: 32rpx;
  box-shadow: 0px -6px 10px rgba(15,39,51,0.1);
}
.new_pass-items {
  position: relative;
  width: 50%;
  text-align: center;
}
.new_pass-items:first-child::after {
  content: "";
  position: absolute;
  top: 10rpx;
  bottom: 10rpx;
  right: 0;
  border-left: 2rpx solid #e5e5e5;
}


.note__voice {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
}

.voice-img {
  align-self: center;
  width: 30rpx;
  height: 30rpx;
}


