.image-plus {
  width: 150px;
  height: 200rpx;
  border: 2rpx solid #D9D9D9;
  position: relative;
  margin: 0 auto
}
.image-plus-nb{
  border: 0;
}
.image-plus-text{
  color: #888888;
  font-size: 18px;
  text-align:center;
}
.image-plus-horizontal {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 4rpx;
  height: 80rpx;
  transform: translate(-50%, -50%);
}
.image-plus-vertical {
  position: absolute;
  top: 50%;
