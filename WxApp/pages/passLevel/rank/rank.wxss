page {
  background: #f4f4f4;
}

.cup-pic {
  width: 180rpx;
  height: 200rpx;
}

.group-tit {
  font-size: 54rpx;
  color: #fff;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 450rpx;
  padding: 84rpx 0 32rpx 0;
}

.score-rankingList__head .tit {
  font-size: 54rpx;
  color: #fff;
  line-height: 1;
  padding: 84rpx 0 32rpx 0;
}

.score-rankingList__head {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  height: 300rpx;
  background-color: #1ea0fa;
  font-size: 28rpx;
  color: rgba(255, 255, 255, 0.6);
  line-height: 1;
  padding: 0 50rpx;
}

.myself-wrap {
  /* position: absolute;
  top: 240rpx;
  left: 20rpx;
  right: 20rpx;
  border-radius: 16rpx; */
}

.myselfRank {
  position: relative;
  padding: 0 30rpx;
  font-size: 34rpx;
  background: #fff;
  height: 134rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  /* border-radius: 16rpx 16rpx 0 0; */
}

.myselfInfo {
  font-size: 26rpx;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 42rpx;
  background-color: #f7faff;
  /* border-radius: 0 0 16rpx 16rpx; */
}

.rank_List {
  font-size: 34rpx;
  background: #fff;
  height: 118rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  border-bottom: 1px solid #e6e6e6;
}

.curRank {
  font-size: 34rpx;
  color: rgba(30, 160, 250, 1);
  flex: 0 0 auto;
  margin-right: 30rpx;
  text-align: center;
}

.cur-rank-info {
  display: flex;
  display: -webkit-flex;
  font-size: 24rpx;
  color: #808080;
}

.rank-box {
  margin-right: 20rpx;
}

.time-icon {
  position: relative;
  top: 2rpx;
  width: 22rpx !important;
  height: 22rpx !important;
  margin-right: 5rpx !important;
}

.score-rankingLis__inner {
  flex: 1 1 auto;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
}

.viewull-left {
  display: flex;
  display: -webkit-flex;
  align-items: center;
}

.viewull-right {
  color: rgba(30, 160, 250, 1);
  font-size: 34rpx;
}

.fs_smail {
  font-size: 28rpx;
}

.viewull-left image, .myPic {
  width: 74rpx;
  height: 74rpx;
  margin-right: 20rpx;
}

.myMess {
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
}

.viewull-left .nametext {
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
  max-width: 380rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.viewull-left .ranktext {
  font-size: 24rpx;
  color: rgba(128, 128, 128, 1);
}

.score-rankingLis__sviewace {
  height: 31rpx;
}

.score-rankingList__score {
  color: rgba(128, 128, 128, 1);
}

.everyone-wrap {
  margin-top: 20rpx;
  /* position: relative;
  margin: 90rpx 20rpx 0;
  box-shadow: 0 0 8rpx rgba(184, 197, 204, 0.1); */
}

.everyone-top-style {
  margin-top: 40rpx !important;
}

.everyone-rank {
  padding: 0 30rpx;
  background: #fff;
  margin-bottom: -2rpx;
  /* border-radius: 16rpx 16rpx 0 0; */
}

.everyone-rank .rank_List:nth-of-type(-n+3) .viewull-right {
  color: #ff941a;
}

.everyone-rank .curRank {
  color: rgba(128, 128, 128, 1);
}

.everyone-rank .rank_List .curRank text {
  display: inline-block;
  width: 54rpx;
  height: 66rpx;
}

.everyone-rank .rank_List:nth-of-type(-n+3) .curRank text {
  display: inline-block;
  text-indent: -999em;
  background-size: cover;
}

.everyone-rank .rank_List:nth-of-type(1) .curRank text {
  background-image: url(http://cgstatic.wjx.cn/images/ranking__one@2x.png);
}

.everyone-rank .rank_List:nth-of-type(2) .curRank text {
  background-image: url(http://cgstatic.wjx.cn/images/ranking__two@2x.png);
}

.everyone-rank .rank_List:nth-of-type(3) .curRank text {
  background-image: url(http://cgstatic.wjx.cn/images/ranking__three@2x.png);
}

.timed {
  font-size: 24rpx;
}

.nodatatext {
  padding-top: 210rpx;
  font-size: 28rpx;
  color: #808080;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.nodatatext image {
  width: 156rpx;
  height: 136rpx;
}

.nodatatext text {
  margin-top: 36rpx;
  color: #b9b9b9;
}

.clearData {
  background: #fafbff;
  font-size: 28rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: rgb(153, 153, 153);
  /* border-radius: 0 0 16rpx 16rpx; */
  border-top: 2rpx solid #e7e7e7;
}

.share__btn {
  position: relative;
  height: 100%;
  font-size: 32rpx;
  line-height: 95rpx;
  background-color: transparent;
}

button::after {
  display: none;
}

.nav-tab {
  display: flex;
  display: -webkit-flex;
  font-size: 28rpx;
  color: #b2dfff;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #008ff2;
  padding: 0 50rpx;
}

.nav-tab view {
  position: relative;
  width: 50%;
  text-align: center;
}

.currented::after {
  color: #fff;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
  content: "";
  width: 0;
  height: 0;
  border-bottom: 6px dashed;
  border-right: 6px solid transparent;
  border-left: 6px solid transparent;
}
